<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Change Material">
		<test name="HeiPort_Login_LDR">
		<classes>
			<class name="com.mendix.test.LoginScript">
				<methods>
				    <include name="tasKill" />
					<include name="openBrowser" />
					<include name="loginAsLDR" />
				</methods> 	
			</class>
		</classes>
	</test>
	<test name="Material_Change_Check">
		<classes>
			<class name="com.mendix.test.MaterialChangeScript">
				<methods>
					<include name="Material_Delete_Record" />
				</methods>
			</class>
		</classes>
	</test>
	 
	
	 <test name="Process_Information_Check">
		<classes>
			<class name="com.mendix.test.MaterialScript">
				<methods>
					<include name="Process_Information_Check_FlagDeletion" />
				</methods>
			</class>
		</classes>
	</test>
	<test name="Material_Create_Syndication_Check">
		<classes>
			<class name="com.mendix.test.MaterialScript">
				<methods>
					<include name="Material_Create_Syndication_Check" />
				</methods>
			</class>
		</classes>
	</test>
	 </suite> 


